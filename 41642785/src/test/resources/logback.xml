<?xml version="1.0" encoding="UTF-8"?>

<configuration>
  <appender class="ch.qos.logback.core.ConsoleAppender" name="message">
    <encoder>
      <pattern>%-5p %m%n</pattern>
    </encoder>
  </appender>

  <root level="ERROR">
    <appender-ref ref="message"/>
  </root>
</configuration>
